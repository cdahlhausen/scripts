[core]
    quotepath = false
      whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
  editor = /usr/bin/vim
[color]
    ui = true
[color "branch"]
    current = yellow black
      local = yellow
        remote = magenta
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = green
    whitespace = white reverse
[color "status"]
    added = yellow
      changed = green
        untracked = cyan reverse
          branch = magenta
[user]
  name = Christian Dahlhausen
  email = dahlhausen@gmail.com
